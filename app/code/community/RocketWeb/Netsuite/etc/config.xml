<config>
    <modules>
        <RocketWeb_Netsuite>
            <version>2.0.6</version>
        </RocketWeb_Netsuite>
    </modules>
    <global>
        <blocks>
            <rocketweb_netsuite>
                <class>RocketWeb_Netsuite_Block</class>
            </rocketweb_netsuite>
            <adminhtml>
                <rewrite>
                    <tax_rate_grid>RocketWeb_Netsuite_Block_Rewrite_Adminhtml_Tax_Rate_Grid</tax_rate_grid>
                    <tax_rate_form>RocketWeb_Netsuite_Block_Rewrite_Adminhtml_Tax_Rate_Form</tax_rate_form>
                </rewrite>
            </adminhtml>
        </blocks>
        <models>
            <rocketweb_netsuite>
                <class>RocketWeb_Netsuite_Model</class>
                <resourceModel>rocketweb_netsuite_resource</resourceModel>
            </rocketweb_netsuite>
            <rocketweb_netsuite_resource>
                <class>RocketWeb_Netsuite_Model_Resource</class>
                <entities>
                    <queue_message>
                        <table>message</table>
                    </queue_message>
                    <apilogitem>
                        <table>netsuite_api_log</table>
                    </apilogitem>
                    <adjustmentinventory>
                        <table>netsuite_adjustment_inventory</table>
                    </adjustmentinventory>
                    <changelog>
                        <table>netsuite_changelog</table>
                    </changelog>
                    <productcost>
                        <table>netsuite_product_cost</table>
                    </productcost>
                    <itembundle>
                        <table>netsuite_shipment_bundle</table>
                    </itembundle>
                </entities>
            </rocketweb_netsuite_resource>
            <tax>
                <rewrite>
                    <calculation>RocketWeb_Netsuite_Model_Rewrite_Tax_Calculation</calculation>
                </rewrite>
            </tax>
            <tax_resource>
                <rewrite>
                    <calculation>RocketWeb_Netsuite_Model_Rewrite_Tax_Resource_Calculation</calculation>
                </rewrite>
            </tax_resource>
        </models>
    	<helpers>
    		<rocketweb_netsuite>
    			<class>RocketWeb_Netsuite_Helper</class>
    		</rocketweb_netsuite>
    	</helpers>
    	<resources>
    		<rocketweb_netsuite_setup>
    			<setup>
    				<module>RocketWeb_Netsuite</module>
    			</setup>
    			<connection>
                    <use>core_setup</use>
                </connection>
    		</rocketweb_netsuite_setup>
    	</resources>
    	<events>
            <customer_save_after>
                <observers>
                    <queue_customer_for_netsuite>
                        <type>singleton</type>
                        <class>rocketweb_netsuite/observer</class>
                        <method>queueCustomerForNetsuite</method>
                    </queue_customer_for_netsuite>
                </observers>
            </customer_save_after>
            <customer_address_save_after>
                <observers>
                    <queue_customer_for_netsuite>
                        <type>singleton</type>
                        <class>rocketweb_netsuite/observer</class>
                        <method>queueCustomerForNetsuiteBasedOnAddress</method>
                    </queue_customer_for_netsuite>
                </observers>
            </customer_address_save_after>
            <customer_delete_after>
                <observers>
                    <queue_customer_delete_for_netsuite>
                        <type>singleton</type>
                        <class>rocketweb_netsuite/observer</class>
                        <method>queueCustomerDelete</method>
                    </queue_customer_delete_for_netsuite>
                    <delete_customer_from_netsuite_export_queue>
                        <type>singleton</type>
                        <class>rocketweb_netsuite/observer</class>
                        <method>deleteCustomerFromExportQueue</method>
                    </delete_customer_from_netsuite_export_queue>
                </observers>
            </customer_delete_after>
            <log_log_clean_after>
                <observers>
                    <clean_api_log>
                        <type>singleton</type>
                        <class>rocketweb_netsuite/observer</class>
                        <method>cleanApiLog</method>
                    </clean_api_log>
                </observers>
            </log_log_clean_after>
            <sales_order_place_after>
                <observers>
                    <queue_order_place_for_netsuite>
                        <type>singleton</type>
                        <class>rocketweb_netsuite/observer</class>
                        <method>queueOrderPlaceForNetsuite</method>
                    </queue_order_place_for_netsuite>
                </observers>
            </sales_order_place_after>
            <catalog_product_save_after>
                <observers>
                    <queue_product_save_for_netsuite>
                        <type>singleton</type>
                        <class>rocketweb_netsuite/observer</class>
                        <method>queueProductSaveForNetsuite</method>
                    </queue_product_save_for_netsuite>
                    <queue_product_save_cost_netsuite>
                        <type>singleton</type>
                        <class>rocketweb_netsuite/observer</class>
                        <method>queueProductSaveCostNetsuite</method>
                    </queue_product_save_cost_netsuite>
                </observers>
            </catalog_product_save_after>
            <sales_order_invoice_save_after>
                <observers>
                    <queue_invoice_save_for_netsuite>
                        <type>singleton</type>
                        <class>rocketweb_netsuite/observer</class>
                        <method>queueInvoiceSaveForNetsuite</method>
                    </queue_invoice_save_for_netsuite>
                </observers>
            </sales_order_invoice_save_after>
    	</events>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
    					<RocketWeb_Netsuite before="Mage_Adminhtml">RocketWeb_Netsuite_Adminhtml</RocketWeb_Netsuite>
    				</modules>
    			</args>
    		</adminhtml>
    	</routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <rocketweb_netsuite>
                    <file>rocketweb_netsuite.xml</file>
                </rocketweb_netsuite>
            </updates>
        </layout>
        <events>
            <core_block_abstract_prepare_layout_before>
                <observers>
                    <add_view_in_netsuite_buttons>
                        <type>singleton</type>
                        <class>rocketweb_netsuite/adminhtml_observer</class>
                        <method>addViewInNetsuiteButtons</method>
                    </add_view_in_netsuite_buttons>
                </observers>
            </core_block_abstract_prepare_layout_before>
        </events>
    </adminhtml>
    <crontab>
    	<jobs>
    		<rocketweb_netsuite_send_warnings>
    			<schedule>
    				<cron_expr>1 1 * * *</cron_expr>
    			</schedule>
    			<run>
    				<model>rocketweb_netsuite/observer::sendWarnings</model>
    			</run>
    		</rocketweb_netsuite_send_warnings>
            <rocketweb_netsuite_clean_api_call_log>
                <schedule>
                    <cron_expr>0 3 * * *</cron_expr>
                </schedule>
                <run>
                    <model>rocketweb_netsuite/observer::cleanApiCallLog</model>
                </run>
            </rocketweb_netsuite_clean_api_call_log>
            <rocketweb_netsuite_clean_changelog>
                <schedule>
                    <cron_expr>30 3 * * *</cron_expr>
                </schedule>
                <run>
                    <model>rocketweb_netsuite/observer::cleanChangelog</model>
                </run>
            </rocketweb_netsuite_clean_changelog>
    	</jobs>
    </crontab>
    <rocketweb_netsuite>
        <netsuite_links>
            <order_view>
                <block_class>Mage_Adminhtml_Block_Sales_Order_View</block_class>
                <magento_object_registry_variable>current_order</magento_object_registry_variable>
                <netsuite_field_id>netsuite_internal_id</netsuite_field_id>
                <netsuite_relative_url><![CDATA[app/accounting/transactions/salesord.nl?id={{ID}}&whence=]]></netsuite_relative_url>
            </order_view>
            <invoice_view>
                <block_class>Mage_Adminhtml_Block_Sales_Order_Invoice_View</block_class>
                <magento_object_registry_variable>current_invoice</magento_object_registry_variable>
                <netsuite_field_id>netsuite_internal_id</netsuite_field_id>
                <netsuite_relative_url><![CDATA[app/accounting/transactions/{{object}}.nl?id={{ID}}&whence=]]></netsuite_relative_url>
            </invoice_view>
            <shipment_view>
                <block_class>Mage_Adminhtml_Block_Sales_Order_Shipment_View</block_class>
                <magento_object_registry_variable>current_shipment</magento_object_registry_variable>
                <netsuite_field_id>netsuite_internal_id</netsuite_field_id>
                <netsuite_relative_url><![CDATA[app/accounting/transactions/itemship.nl?id={{ID}}&whence=]]></netsuite_relative_url>
            </shipment_view>
            <creditmemo_view>
                <block_class>Mage_Adminhtml_Block_Sales_Order_Creditmemo_View</block_class>
                <magento_object_registry_variable>current_creditmemo</magento_object_registry_variable>
                <netsuite_field_id>netsuite_internal_id</netsuite_field_id>
                <netsuite_relative_url><![CDATA[app/accounting/transactions/custcred.nl?id={{ID}}]]></netsuite_relative_url>
            </creditmemo_view>
        </netsuite_links>
