    </rocketweb_netsuite>
    <default>
    	<rocketweb_netsuite>
    		<general>
    			<host>https://webservices.netsuite.com</host>
    			<end_point>2013_1</end_point>
    			<role_id>3</role_id>
                <soap_request_timeout>1800</soap_request_timeout>
    		</general>
            <connection_import>
                <same>1</same>
            </connection_import>
            <connection_export>
                <same>1</same>
            </connection_export>
            <connection_stock>
                <same>1</same>
            </connection_stock>
    		<queue_processing>
    			<export_batch_size>50</export_batch_size>
    			<import_batch_size>5000</import_batch_size>
    			<timeout>180</timeout>
                <updated_from_minutes>1140</updated_from_minutes>
    		</queue_processing>
            <orders>
                <set_tran_id>1</set_tran_id>
                <default_status_map>
                    <status_new>
                        <netsuite_status>_pendingApproval</netsuite_status>
                        <magento_status>new</magento_status>
                    </status_new>
                    <status_pending_payment>
                        <netsuite_status>_pendingBilling</netsuite_status>
                        <magento_status>pending_payment</magento_status>
                    </status_pending_payment>
                    <status_processing>
                        <netsuite_status>_pendingFulfillment</netsuite_status>
                        <magento_status>processing</magento_status>
                    </status_processing>
                    <status_billed>
                        <netsuite_status>_fullyBilled</netsuite_status>
                        <magento_status>complete</magento_status>
                    </status_billed>
                    <status_closed>
                        <netsuite_status>_closed</netsuite_status>
                        <magento_status>closed</magento_status>
                    </status_closed>
                    <status_canceled>
                        <netsuite_status>_cancelled</netsuite_status>
                        <magento_status>canceled</magento_status>
                    </status_canceled>
                    <status_pendingbillingpartfulfilled>
                        <netsuite_status>_pendingBillingPartFulfilled</netsuite_status>
                        <magento_status>processing</magento_status>
                    </status_pendingbillingpartfulfilled>
                    <status_pendingfulfill>
                        <netsuite_status>_partiallyFulfilled</netsuite_status>
                        <magento_status>processing</magento_status>
                    </status_pendingfulfill>
                </default_status_map>
                <invoices_netsuite_type>CashSale</invoices_netsuite_type> <!--TYPE_CASH_SALE-->
            </orders>
    		<developer>
    			<export_queue_threshold>100</export_queue_threshold>
    			<import_queue_threshold>100</import_queue_threshold>
                <clean_api_call_log_after>30</clean_api_call_log_after>
                <changelog_enabled>1</changelog_enabled>
                <changelog_lifetime>30</changelog_lifetime>
    		</developer>
    		<products>
    			<netsuite_link_field>externalIdString</netsuite_link_field>
    			<magento_link_field>id</magento_link_field>
                <sync_product_images>1</sync_product_images>
                <default_visibility>4</default_visibility> <!--Mage_Catalog_Model_Product_Visibility::VISIBILITY_BOTH-->
                <default_status>1</default_status><!--Mage_Catalog_Model_Product_Status::STATUS_ENABLED-->
                <default_tax_class_id>0</default_tax_class_id>
                <default_website_ids>1</default_website_ids>
                <default_store_ids>1</default_store_ids>
                <image_sync_on_update>1</image_sync_on_update>
                <default_attribute_set_id>4</default_attribute_set_id> <!--This is usually the "Default" attribute set-->
                <tier_price_website>0</tier_price_website>
                <tier_price_customer_group>32000</tier_price_customer_group> <!-- Mage_Customer_Model_Group::CUST_GROUP_ALL -->
                <default_field_map>
                    <name_map>
                        <magento>name</magento>
                        <netsuite_field_type>standard_field</netsuite_field_type>
                        <netsuite>storeDisplayName</netsuite>
                    </name_map>
                    <sku_map>
                        <magento>sku</magento>
                        <netsuite_field_type>standard_field</netsuite_field_type>
                        <netsuite>mpn</netsuite>
                    </sku_map>
                    <description_map>
                        <magento>short_description</magento>
