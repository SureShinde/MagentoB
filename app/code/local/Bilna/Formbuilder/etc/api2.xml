<?xml version="1.0"?>
<config>
    <api2>
        <resource_groups>
            <bilna_formbuilder translate="title" module="Bilna_Formbuilder">
                <title>Bilna FormBuilder</title>
                <sort_order>1010</sort_order>
            </bilna_formbuilder>
        </resource_groups>
        <resources>
            <bilna_formbuilder_form translate="title" module="Bilna_Formbuilder">
                <group>bilna_formbuilder</group>
                <model>bilna_formbuilder/api2_form</model>
                <title>Form</title>
                <sort_order>10</sort_order>
                <privileges>
                    <admin>
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>
                <attributes translate="title static_info success_message static_success static_failed force_flow url termsconditions freeproducts active_from active_to class button_text sent_email email_id status success_redirect url_success email_share_apps product_promo social_title social_desc social_image fue" module="Bilna_Formbuilder">
                    <title>Title</title>
                    <static_info>Static Info</static_info>
                    <success_message>Success Message</success_message>
                    <static_success>Static Success</static_success>
                    <static_failed>Static Failed</static_failed>
                    <force_flow>Force Flow</force_flow>
                    <url>Url</url>
                    <termsconditions>Terms and Condition</termsconditions>
                    <freeproducts>Free Products</freeproducts>
                    <active_from>Active From</active_from>
                    <active_to>Active To</active_to>
                    <class>Class</class>
                    <button_text>Button Text</button_text>
                    <sent_email>Sent Email</sent_email>
                    <email_id>Email Id</email_id>
                    <status>Status</status>
                    <success_redirect>Success Redirect</success_redirect>
                    <url_success>Url Success</url_success>
                    <email_share_apps>Email Share Apps</email_share_apps>
                    <product_promo>Product Promo</product_promo>
                    <social_title>Social Title</social_title>
                    <social_desc>Social Description</social_desc>
                    <social_image>Social Image</social_image>
                    <fue>FUE</fue>
                </attributes>
                <!--exclude_attributes>
                    <admin>
                        <read>
                            <id>1</id>
                        </read>
                    </admin>
                </exclude_attributes-->
                <routes>
                    <route_entity>
                        <route>/formbuilder/form</route>
                        <action_type>entity</action_type>
                    </route_entity>
                </routes>
                <versions>1</versions>
            </bilna_formbuilder_form>
            <bilna_formbuilder_input translate="title" module="Bilna_Formbuilder">
                <group>bilna_formbuilder</group>
                <model>bilna_formbuilder/api2_input</model>
                <title>Input</title>
                <sort_order>20</sort_order>
                <privileges>
                    <admin>
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>
                <attributes translate="id name group title value helper_message validation type dbtype required unique order" module="Bilna_Formbuilder">
                    <id>ID</id>
                    <name>Name</name>
                    <group>Group</group>
                    <title>Title</title>
                    <value>Value</value>
                    <helper_message>Helper Message</helper_message>
                    <validation>Validation</validation>
                    <type>Type</type>
                    <dbtype>DB Type</dbtype>
                    <required>Required</required>
                    <unique>Unique</unique>
                    <order>Order</order>
                </attributes>
                <routes>
                    <route_entity>
                        <route>/formbuilder/input/:formid</route>
                        <action_type>entity</action_type>
                    </route_entity>
                </routes>
                <versions>1</versions>
            </bilna_formbuilder_input>
        </resources>
    </api2>
</config>