<?xml version="1.0"?>
<config>
    <modules>
        <Mage_CatalogInventoryLocal>
            <version>1.6.0.0.3</version>
        </Mage_CatalogInventoryLocal>
    </modules>
    <global>
        <models>
            <cataloginventory>
                <rewrite>
                    <observer>Mage_CatalogInventoryLocal_Model_Observer</observer>
                    <stock>Mage_CatalogInventoryLocal_Model_Stock</stock>
                    <stock_item>Mage_CatalogInventoryLocal_Model_Stock_Item</stock_item>
                </rewrite>
            </cataloginventory>
        </models>
    </global>
</config>
